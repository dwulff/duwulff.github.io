result = lapply(rat_list,function(d){
gr = d[,c('leftWord','rightWord','wert')]
gr$wert = (gr$wert - min(gr$wert)) / max(gr$wert - min(gr$wert))
res= data.frame(t(sapply(seq(0,.6,.1),function(x){
gra = graph.data.frame(gr[gr[,3]>=x,1:2],directed = F)
ned = length(E(gra))
nno = length(V(gra))
k = ned/nno
e_spl = log(nno)/log(k)
e_clu = k/(nno)
clu = transitivity(gra,'localaverage')
spl = average.path.length(gra)
c(
clu,
e_clu,
clu/e_clu,
spl,
e_spl,
spl/e_spl,
(clu/e_clu) / (spl/e_spl)
)
})))
names(res) = c('clu','e_clu','c_clu','spl','e_spl','c_spl','s')
res = data.frame('lab'=d$labels[1],'group'=substr(d$labels[1],1,1)[[1]],'lim'=seq(0,.6,.1),res)
return(res)
})
